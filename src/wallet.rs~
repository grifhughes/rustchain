extern crate hyper;

struct Wallet {
    guid: String,
    main_password: String,
    two_factor_auth: char,
    secondary_password: Option<String>,
    
}

impl Wallet {
    fn new(&self, g: &str, mp: &str, tfa: char, sp: &str) -> Box<Wallet> {
       Box::new(Wallet {
            guid: g.to_string(),
            main_password: mp.to_string(),
            two_factor_auth: tfa,
            secondary_password: sp.to_string()
        })
    }
}


